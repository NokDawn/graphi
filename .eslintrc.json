{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        // "semi": [2, "never"],
        "import/prefer-default-export": 0,
        "import/extensions": [2, "always"],
        "no-console": "warn"
    }
}
